{
  "manifest_version": 3,
  "name": "Cookie Cleaner",
  "version": "4",
  "description": "Clear cookies and other site data",
  "permissions": ["browsingData", "storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
