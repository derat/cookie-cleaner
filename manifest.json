{
  "manifest_version": 2,
  "name": "Cookie Cleaner",
  "version": "3",
  "description": "Clear cookies and other site data",
  "permissions": ["browsingData", "storage"],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "persistent": false,
    "scripts": ["common.js", "background.js"]
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
